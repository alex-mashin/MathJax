			"rem",
			"root",
			"scalarproduct",
			"sdev",
			"sec",
			"sech",
			"selector",
			"semantics",
			"sep",
			"set",
			"setdiff",
			"sin",
			"sinh",
			"subset",
			"sum",
			"tan",
			"tanh",
			"tendsto",
			"times",
			"transpose",
			"true",
			"union",
			"uplimit",
			"variance",
			"vector",
			"vectorproduct",
			"xor"
		],
		"AddWikilinks": false
	}
}
